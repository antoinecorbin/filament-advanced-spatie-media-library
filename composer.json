{
  "name": "antoinecorbin/filament-advanced-spatie-media-library",
  "description": "Allows you to manage media translations with Spatie Translatable and Filament repeater support.",
  "license": "MIT",
  "authors": [
    {
      "name": "Antoine Corbin",
      "email": "acorbin.pro@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/lara-zeus/translatable"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.45|^11.0",
    "filament/spatie-laravel-media-library-plugin": "^3.2",
    "spatie/laravel-medialibrary": "^10.0|^11.0",
    "spatie/laravel-package-tools": "^1.19"
  },
  "autoload": {
    "psr-4": {
      "AntoineCorbin\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
